    GC0310_SERIALSensor.sccb_opened = KAL_FALSE; // lanking 20120423
    CisModulePowerOn(GC0310_SERIALSensor.SensorIdx, KAL_FALSE);
    CameraSccbClose(CAMERA_SCCB_SENSOR);

    //Resume
    #if defined(MT6250)
    GPIO_ModeSetup(52, 1);
    GPIO_ModeSetup(64, 1);
    #else    // MT6252
    GPIO_ModeSetup(47, 1);
    GPIO_ModeSetup(59, 1);
    #endif
 

    return MM_ERROR_NONE;
}

